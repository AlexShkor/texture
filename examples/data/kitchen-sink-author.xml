<?xml version="1.0" encoding="UTF-8"?>
<!-- Kitchen sink with a reduced strict set of JATS tags -->
<!DOCTYPE article PUBLIC "-//NLM//DTD JATS (Z39.96) Journal Archiving and Interchange DTD v1.1d3 20150301//EN"  "JATS-archivearticle1.dtd">
<article article-type="sample" dtd-version="1.1d3" xmlns:xlink="http://www.w3.org/1999/xlink">
  <front>
    <article-meta>
      <title-group>
        <article-title>Kitchen sink <italic>example</italic> for authors</article-title>
      </title-group>

      <!-- only first contrib-group considered in author mode -->
      <contrib-group>
        <contrib contrib-type="author" id="author-1">
          <name>
            <surname>Doe</surname>
            <given-names>John</given-names>
          </name>
          <bio>
            <p>John Doe biography</p>
          </bio>
        </contrib>
        <contrib contrib-type="author" id="author-1">
          <name>
            <surname>Doe</surname>
            <given-names>Jane</given-names>
          </name>
          <bio>
            <p>Jane Doe biography</p>
          </bio>
        </contrib>
      </contrib-group>

      <!-- only one abstract element is considered in Author mode -->
      <abstract>
        <p>This is an <bold>article abstract</bold>, with a single paragraph</p>
      </abstract>
    </article-meta>
  </front>
  <body>
    <sec id="s1">
      <title>First Section</title>
      <p id="p1">This is a basic <bold>JATS-compatible</bold> article, using a reduced set of tags. It supports citing references (<xref ref-type="bibr" rid="bib1 bib2">[1,2]</xref>) and figures.</p>
    </sec>
    <sec id="s2">
      <title>Second Section</title>
      <p id="p2">This is the second paragraph</p>
    </sec>
  </body>
